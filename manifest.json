{
  "manifest_version": 2,
  "name": "automation",
  "description": "Add automation extra time",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "icon.png"
  ],
  "permissions": [
    "http://localhost:7253/",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "http://automation.1st.co.com:8888/*"
      ]
    }
  ]
}